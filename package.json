{
  "name": "react-viz-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "d3": "^5.9.1",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.16.0",
    "redux": "^4.0.0",
    "styled-components": "^4.1.2"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:server start:client",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "start:server": "json-server --watch data.json --port 3004",
    "start:client": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/d3": "^5.7.1",
    "@types/jest": "^23.1.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.5.1",
    "@types/react": "^16.4.6",
    "@types/react-datepicker": "^1.1.5",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.3",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "4.0.3",
    "json-server": "^0.14.0",
    "npm-run-all": "^4.1.3",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  }
}
